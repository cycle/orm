{
  "name": "cycle/orm",
  "type": "library",
  "license": "MIT",
  "description": "PHP DataMapper ORM and Data Modelling Engine",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.0",
    "ext-pdo": "*",
    "doctrine/inflector": "^1.4|^2.0",
    "doctrine/instantiator": "^1.3.1",
    "laminas/laminas-hydrator": "^4.0",
    "cycle/database": "dev-master as 1.0.0"
  },
  "require-dev": {
    "doctrine/collections": "^1.6",
    "illuminate/collections": "^8.0",
    "mockery/mockery": "^1.1",
    "phpunit/phpunit": "^9.5",
    "ramsey/uuid": "^4.0",
    "spiral/code-style": "^1.0",
    "spiral/tokenizer": "^2.8",
    "vimeo/psalm": "^4.9"
  },
  "autoload": {
    "psr-4": {
      "Cycle\\ORM\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cycle\\ORM\\Tests\\": "tests/ORM/"
    }
  }
}
