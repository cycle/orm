{
  "name": "cycle/orm",
  "type": "library",
  "license": "MIT",
  "description": "PHP DataMapper ORM and Data Modelling Engine",
  "require": {
    "php": "^7.2",
    "ext-pdo": "*",
    "spiral/database": "^2.3",
    "doctrine/inflector": "^1.3",
    "doctrine/collections": "^1.5",
    "doctrine/instantiator": "^1.2",
    "zendframework/zend-hydrator": "^2.4|^3.0"
  },
  "require-dev": {
    "spiral/tokenizer": "^2.1",
    "phpunit/phpunit": "~8.0",
    "mockery/mockery": "^1.1",
    "spiral/dumper": "^1.0",
    "ramsey/uuid": "^3.8",
    "spiral/code-style": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Cycle\\ORM\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cycle\\ORM\\Tests\\": "tests/ORM/"
    }
  }
}
